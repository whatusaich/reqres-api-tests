	"item": [
		{
			"name": "GET users page 2 (TC-01)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Общие проверки\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Content-Type is application/json\", function () {\r",
							"    pm.response.to.have.header(\"content-type\");\r",
							"    pm.expect(pm.response.headers.get(\"content-type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"// Парсим JSON\r",
							"let jsonData = pm.response.json();\r",
							"\r",
							"// Проверяем, что page = 2\r",
							"pm.test(\"page equals 2\", function () {\r",
							"    pm.expect(jsonData.page).to.eql(2);\r",
							"});\r",
							"\r",
							"// data — массив и не пустой\r",
							"pm.test(\"data is an array and not empty\", function () {\r",
							"    pm.expect(jsonData.data).to.be.an(\"array\").that.is.not.empty;\r",
							"});\r",
							"\r",
							"// Проверяем, что в каждом объекте есть ключи id, email, first_name, last_name, avatar\r",
							"pm.test(\"each user has id, email, first_name, last_name, avatar\", function () {\r",
							"    jsonData.data.forEach(user => {\r",
							"        pm.expect(user).to.have.property(\"id\");\r",
							"        pm.expect(user).to.have.property(\"email\");\r",
							"        pm.expect(user).to.have.property(\"first_name\");\r",
							"        pm.expect(user).to.have.property(\"last_name\");\r",
							"        pm.expect(user).to.have.property(\"avatar\");\r",
							"    });\r",
							"});\r",
							"\r",
							"// Проверка времени ответа (опционально)\r",
							"pm.test(\"response time is less than 2000 ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "https://reqres.in/api/users?page=2",
					"protocol": "https",
					"host": [
						"reqres",
						"in"
					],
					"path": [
						"api",
						"users"
					],
					"query": [
						{
							"key": "page",
							"value": "2"
						}
					]
				}
			},
			"response": []
		},
